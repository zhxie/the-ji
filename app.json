{
  "expo": {
    "name": "The JI",
    "slug": "the-ji",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "the-ji",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.ratethefilm.ji",
      "buildNumber": "1",
      "config": {
        "usesNonExemptEncryption": false
      },
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "package": "com.ratethefilm.ji",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6546fb1b-58e1-40f2-bbb9-391f13909289"
      }
    }
  }
}
